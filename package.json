{
  "name": "flow-to-typescript",
  "description": "Compile Flow to TypeScript",
  "version": "0.0.1",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "flow2ts": "dist/src/cli.js"
  },
  "repository": "git@github.com:bcherny/flow-to-typescript.git",
  "author": "Boris Cherny <bcherny@fb.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "rm -rf ./dist",
    "lint": "tslint src/**/*.ts",
    "prepublishOnly": "npm run clean && npm run lint && npm run build -- -d",
    "pretest": "npm run build",
    "tdd": "concurrently -k 'npm run build:watch' 'npm run test:watch'",
    "test": "ava --verbose",
    "test:debug": "node --inspect-brk ./node_modules/ava/profile.js ./dist/test/test.js",
    "test:watch": "ava -w"
  },
  "dependencies": {
    "@babel/generator": "^7.0.0-beta.54",
    "@babel/traverse": "^7.0.0-beta.54",
    "babel": "^6.23.0",
    "glob": "^7.1.2",
    "lodash": "^4.17.10",
    "mz": "^2.7.0"
  },
  "devDependencies": {
    "@types/glob": "^5.0.35",
    "@types/lodash": "^4.14.112",
    "@types/minimist": "^1.2.0",
    "@types/mz": "^0.0.32",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "concurrently": "^3.6.0",
    "flow-bin": "^0.76.0",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2"
  },
  "ava": {
    "files": [
      "./dist/test/test.js"
    ]
  }
}
